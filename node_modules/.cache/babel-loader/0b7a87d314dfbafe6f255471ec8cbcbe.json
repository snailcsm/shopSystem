crumb separator-class=\"el-icon-arrow-right\">\r\n    <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n    <el-breadcrumb-item :to=\"{ path: '/rights' }\">权限管理</el-breadcrumb-item>\r\n    <el-breadcrumb-item>权限列表</el-breadcrumb-item>\r\n  </el-breadcrumb>\r\n\r\n  <div class=\"page-contain\">\r\n    <!--分类tab-->\r\n    <el-tabs v-model=\"tabActiveName\"\r\n             type=\"card\"\r\n             @tab-click=\"changeRightTab\"\r\n             tab-position=\"top\"\r\n    >\r\n      <el-tab-pane label=\"全部\" name=\"all\">全部</el-tab-pane>\r\n      <el-tab-pane label=\"一级权限\" name=\"first\">一级权限</el-tab-pane>\r\n      <el-tab-pane label=\"二级权限\" name=\"second\">二级权限</el-tab-pane>\r\n      <el-tab-pane label=\"三级权限\" name=\"third\">三级权限</el-tab-pane>\r\n    </el-tabs>\r\n    <!--权限列表-->\r\n    <el-table\r\n      class=\"rights-table\"\r\n      :data=\"rightData\"\r\n      border\r\n      style=\"width: 100%\">\r\n      <el-table-column\r\n        label=\"序号\"\r\n        type=\"index\"\r\n        width=\"80\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"authName\"\r\n        label=\"权限名称\"\r\n       >\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"leval\"\r\n        label=\"等级\"\r\n        width=\"360\"\r\n        >\r\n        <template slot-scope=\"scope\">\r\n        <el-tag v-if=\"scope.row.leval===1\">一级</el-tag>\r\n        <el-tag type=\"success\" v-else-if=\"scope.row.leval==2\"> 二级</el-tag>\r\n        <el-tag type=\"warning\" v-else>三级</el-tag>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"Rights\",\r\n      data(){\r\n        return{\r\n          serverData:[],\r\n          rightData:[],\r\n          tabActiveName:'all',\r\n        }\r\n      },\r\n      created() {\r\n        this.getRightData();\r\n      },\r\n      methods:{\r\n        //发送网络请求获取角色数据\r\n        getRightData(){\r\n          this.$http.get('/rights').then((res=>{\r\n            console.log(res.data)\r\n            this.rightData=res.data;\r\n            this.serverData=res.data;\r\n          }))\r\n        },\r\n        changeRightTab(tab){\r\n          if(tab.index==0){\r\n           return  this.rightData=this.serverData;\r\n          }\r\n          this.rightData=this.serverData.filter(item=>{\r\n            return item.leval==tab.index;\r\n          })\r\n        }\r\n      }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n  .rights-table{\r\n    margin-top: -1px;\r\n  }\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/rights"}]}